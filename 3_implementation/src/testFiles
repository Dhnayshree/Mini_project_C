#include "unity.h"
#include "Ceaser_cipher.h"
#include "DES_Cipher.h"

	void setUp()
	{	}
	void tearDown()
	{	}
	
	void test_Generate_Secret_Key_1(void)
	{
  		TEST_ASSERT_GREATER_THAN(-1, Generate_Secret_key_1());
  		TEST_ASSERT_LESS_THAN(10, Generate_Secret_Key_1()20));
	}

    void test_Generate_Secret_Key_2(void)
    {
        TEST_ASSERT_GREATER_THAN_CHAR('\0', Generate_Secret_Key_2);
    }

	int test_main(void)
	{
	  	UNITY_BEGIN();
  	
	  	RUN_TEST(test_Generate_Secret_Key_1);
        
        RUN_TEST(test_Generate_Secret_Key_2);
        

  		return UNITY_END();
	}